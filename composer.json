{
  "name": "nahid-ferdous/laravel-module-generator",
  "description": "Speeds up Laravel development by automating repetitive tasks. This package helps to generate module files (service, controller, model, migration, resource, request, collection) from YAML file.",
  "version": "1.2.16",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "laravel package",
    "module generator",
    "code generator",
    "crud generator",
    "yaml",
    "scaffolding",
    "laravel scaffolding",
    "laravel module",
    "controller generator",
    "model generator",
    "migration generator",
    "request generator",
    "resource generator",
    "service layer",
    "laravel development tools",
    "laravel artisan",
    "php code generation",
    "laravel automation"
  ],
  "authors": [
    {
      "name": "Nahid Ferdous",
      "email": "nahidferdous240@gmail.com",
      "homepage": "https://nahidferdous.netlify.app/",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/nahidnfr123/laravel-module-generator",
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "symfony/yaml": "^6.0 || ^7.0"
  },
  "suggest": {
    "spatie/laravel-permission": "Allows to generate permission controller, model, migration, resource, request, collection"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "NahidFerdous\\LaravelModuleGenerator\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "NahidFerdous\\LaravelModuleGenerator\\Providers\\LaravelModuleGeneratorServiceProvider"
      ]
    }
  }
}
